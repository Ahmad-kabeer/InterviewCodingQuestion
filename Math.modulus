public class C {            //Math.Modulus {|-1|==1} function implementation 
    static int  met(int a){
        if(a<0){
            a=-a;
            return a;
        }
        else if(a==0){
            return a;
        }
        else {
            return a;
        }
    }
    public static void main(String[] arg){
        System.out.println(C.met(-242));;
    }
}
